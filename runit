HOST=`uname -s`
if [ $HOST = 'Darwin' ]
then
#echo $HOST
source ../pyOrbit/customEnvironment.sh 1>/dev/null
elif [ $HOST = 'Linux' ]
then
    export  pyALCELI_ROOT=~/pyAlceli
    export SIMULINAC_ROOT=~/SIMULINAC
    export   pyORBIT_ROOT=~/pyOrbit
    echo $HOST $SIMULINAC_ROOT $pyORBIT_ROOT $pyALCELI_ROOT

    export PYTHONPATH=$SIMULINAC_ROOT:$PYTHONPATH

    if [ -a $pyORBIT_ROOT/activate ]
    then
    source $pyORBIT_ROOT/activate
    else
    source $pyORBIT_ROOT/setupEnvironment.sh
    fi
fi
./START.sh acLinac.py 1
