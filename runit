HOST=`uname -s`
if [ $HOST = 'Darwin' ]
then
#echo $HOST
source ../pyOrbit/customEnvironment.sh 1>/dev/null
elif [ $HOST = 'Linux' ]
then
#echo $HOST
    if [ -a ../pyOrbit/activate ]
    then
    source ../pyOrbit/activate
    else
    source ../pyOrbit/setupEnvironment.sh
    fi
fi
./START.sh acLinac.py 1
